<?xml version="1.0"?>
<package format="2">
  <!--
   This file is mostly here to provide ros dependencies when this project is built with catkin
   -->
  <name>flask_cors</name>
  <version>3.0.2</version>
  <description>
  Cross Origin Resource Sharing ( CORS ) support for Flask
  </description>

  <license>BSD</license>

  <url type="repository">https://github.com/corydolphin/flask-cors.git</url>
  <url type="bugtracker">https://github.com/corydolphin/flask-cors.git/issues</url>

  <author>Cory Dolphin</author>
  <maintainer email="asmodehn@gmail.com">AlexV</maintainer>

  <buildtool_depend version_gte="0.6.18">catkin</buildtool_depend>
  <build_depend version_gte="0.2.0">catkin_pip</build_depend>

  <depend version_gte="0.10.1">python-flask</depend>
  <depend version_gte="1.5.2">python-six</depend>

  <export>
    <rosdoc external_url="http://flask-cors.corydolphin.com/en/latest/" />
  </export>

</package>


